generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model posts {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  coverUrl String
  editorjs String?
  markdown String
  slug     String
  title    String
}
